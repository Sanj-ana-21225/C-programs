#include <stdio.h>
int Prime(int n);  
int main() 
{
    int num;

    printf("Enter a number: ");
    scanf("%d", &num);

    if (count==2)
        printf("%d is a prime number\n", num);
    else
        printf("%d is not a prime number\n", num);

    return 0;
}
int Prime(int n)
{
    int i,count=0;
    for(i=1; i<=n; i++){
    if(n % i==0){
     count++;
   }
    return count;

}
