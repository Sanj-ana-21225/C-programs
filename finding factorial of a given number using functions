    fact = factorial(num);  

    printf("Factorial of %d = %d\n", n, fact);

    return 0;
}
int factorial(int n)
{
    int i, res = 1;

    for (i = 1; i <= n; i++) {
        res = res * i;  
    }

    return res;
}
