#include <stdio.h>

int main()
{
    char str[100];
    int i;
    printf("Enter a string: ");
    scanf("%s", str); 
    for (i = 0; str[i] != '\0'; i++) {
       (str[i] >= 'a' && str[i] <= 'z') {
            str[i] = str[i] - 32;
        }
    }

    printf("Converted string into upper case is: %s\n", str);

    return 0;
}
