#include <stdio.h>
void readmatrix(int a[2][2]);
void printmatrix(int a[2][2]);
int main()
{
    int a[2][2];
    readmatrix(a);
    printmatrix(a);
    return 0;
}

void readmatrix(int a[2][2])
{
    int i, j;
    for(i = 0; i < 2; i++)
    {
        for(j = 0; j < 2; j++)
        {
            scanf("%d", &a[i][j]);
        }
    }
}

void printmatrix(int a[2][2])
{
    int i, j;
    for(i = 0; i < 2; i++)
    {
        for(j = 0; j < 2; j++)
        {
            printf("%d ", a[i][j]);
        }
        printf("\n");
    }
}
